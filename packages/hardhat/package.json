{
  "name": "fhevm-rock-paper-scissors-hardhat",
  "description": "FHEVM Rock Paper Scissors Hardhat",
  "version": "0.1.0",
  "private": true,
  "license": "BSD-3-Clause-Clear",
  "files": [
    "contracts"
  ],
  "homepage": "https://github.com/germartinez/fhevm-rock-paper-scissors/blob/main/packages/hardhat/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/germartinez/fhevm-rock-paper-scissors.git"
  },
  "scripts": {
    "clean": "rimraf ./fhevmTemp ./artifacts ./cache ./types ./dist",
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "lint": "pnpm lint:sol && pnpm lint:ts && pnpm prettier:check",
    "lint:sol": "solhint --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:ts": "eslint --ext .js,.ts . --ignore-path ../../.eslintignore",
    "postcompile": "pnpm typechain",
    "prettier:check": "prettier --check \"**/*.{js,json,md,sol,ts,yml}\" --ignore-path ../../.prettierignore",
    "prettier:write": "prettier --write \"**/*.{js,json,md,sol,ts,yml}\" --ignore-path ../../.prettierignore",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain && rm -rf ./sdk/src/types && cp -r ./types ../sdk/src",
    "chain": "hardhat node --network hardhat --no-deploy",
    "deploy:localhost": "hardhat deploy --network localhost",
    "deploy:sepolia": "hardhat deploy --network sepolia",
    "verify:sepolia": "hardhat verify --network sepolia"
  },
  "dependencies": {
    "encrypted-types": "^0.0.4",
    "@fhevm/solidity": "^0.8.0",
    "@zama-fhe/oracle-solidity": "^0.1.0"
  },
  "overrides": {
    "ws@>=7.0.0 <7.5.10": ">=7.5.10",
    "axios@>=1.3.2 <=1.7.3": ">=1.7.4",
    "elliptic@>=4.0.0 <=6.5.6": ">=6.5.7",
    "elliptic@>=2.0.0 <=6.5.6": ">=6.5.7",
    "elliptic@>=5.2.1 <=6.5.6": ">=6.5.7",
    "micromatch@<4.0.8": ">=4.0.8",
    "elliptic@<6.6.0": ">=6.6.0",
    "elliptic@<6.5.6": ">=6.5.6",
    "undici@>=6.0.0 <6.21.1": ">=6.21.1",
    "undici@>=4.5.0 <5.28.5": ">=5.28.5",
    "elliptic@<=6.6.0": ">=6.6.1",
    "tar-fs@>=2.0.0 <2.1.2": ">=2.1.2",
    "axios@>=1.0.0 <1.8.2": ">=1.8.2",
    "axios@<0.29.1": ">=0.29.1",
    "cookie@<0.7.0": ">=0.7.0",
    "minimatch": "^3.1.2"
  }
}
